{
  "manifest_version": 3,
  "name": "DeepSeek翻译助手",
  "version": "1.0",
  "description": "使用DeepSeek API进行中英文翻译",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.svg"
  },
  "icons": {
    "16": "icons/icon.svg",
    "32": "icons/icon.svg",
    "48": "icons/icon.svg",
    "128": "icons/icon.svg"
  },
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://api.deepseek.com/*"
  ]
}